    "adm-zip": "^0.5.4",
    "aes-ecb": "^1.3.10",
    "aws-sdk": "^2.1257.0",
    "axios": "^0.21.4",
    "bcryptjs": "^2.4.3",
    "bee-queue": "^1.3.1",
    "bull": "^3.29.2",
