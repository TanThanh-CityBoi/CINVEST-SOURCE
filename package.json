{
  "name": "cinvest-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node --experimental-modules --es-module-specifier-resolution=node ./dist/index.js",
    "start:worker": "node ./dist/worker/index.js",
    "postinstall": "patch-package",
    "pm2": "pm2 start pm2.config.js",
    "pm2:runtime": "pm2-runtime pm2.config.js",
    "pm2:stop": "pm2 stop pm2.config.js",
    "pm2:delete": "pm2 delete pm2.config.js",
    "dev": "ts-node-dev --transpile-only -r dotenv/config src/index.ts",
    "dev:worker": "ts-node-dev --transpile-only -r dotenv/config src/worker/index.ts",
    "lint": "eslint . --ext js,ts",
    "build": "rimraf dist && tsc",
    "typeorm": "npm run build && npx typeorm -d ./dist/config/ormconfig.js",
    "migrate:create": "npm run typeorm -- migration:create",
    "migrate:gen": "npm run typeorm -- migration:generate",
    "migrate:up": "npm run typeorm -- migration:run",
    "migrate:down": "npm run typeorm -- migration:revert",
    "start:watch": "nodemon",
    "prepare": "husky install",
    "start:dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "proto:gen": "rm -rf ./src/proto/types && proto-loader-gen-types --longs=String --enums=String --defaults --keepCase --oneofs --grpcLib=@grpc/grpc-js --outDir=./src/proto/types ./src/proto/proto/*.proto",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
